{
    "name": "t3docs/examples",
    "type": "typo3-cms-extension",
    "description": "This extension packages a number of code examples from the Core Documentation.",
    "license": [
        "GPL-2.0+"
    ],
    "authors": [
        {
            "name": "Documentation Team",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "T3docs\\Examples\\": "Classes/"
        }
    },
    "require": {
        "typo3/cms-core": "^9.5.23"
    },
    "replace": {
        "typo3-ter/examples": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "examples"
        },
        "branch-alias": {
            "dev-9.5": "9.5.x-dev"
        }
    },
    "scripts": {
        "prepare-release": [
            "rm -rf .github",
            "rm .gitignore",
            "rm .editorconfig"
        ]
    }
}
