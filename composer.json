{
    "name": "t3docs/examples",
    "type": "typo3-cms-extension",
    "description": "This extension packages a number of code examples from the Core Documentation.",
    "license": [
        "GPL-2.0+"
    ],
    "authors": [
        {
            "name": "Documentation Team",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "T3docs\\Examples\\": "Classes/"
        }
    },
    "require": {
        "typo3/cms-core": "^11.5.0",
		"typo3/cms-fluid-styled-content": "^11.5.0",
		"typo3/cms-linkvalidator": "^11.5.0"
    },
    "replace": {
        "typo3-ter/examples": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "examples"
        },
        "branch-alias": {
            "dev-master": "11.0.x-dev"
        }
    },
    "scripts": {
        "prepare-release": [
            "rm -rf .github",
            "rm .gitignore",
            "rm .editorconfig"
        ]
    },
    "config": {
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        }
    }
}
